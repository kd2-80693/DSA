package solution02;

import java.util.List;

public class Main {
	public static int linearSearch(List<Employee> list , Employee key )
	{
		for(int i = 0 ; i  < list.size() ; i ++ )
		{
			if(list.get(i).getEmpid()==key.getEmpid())
			{
				System.out.println("Found at "+ i);
				return i;
			}
			
		}
		return -1;
	}
	
	public static int  binarySearch(int[] arr , int key )
	{
		int count = 0 ;
		int left = 0;
		int right = arr.length-1;
		int mid ;
		while(left <= right)
		{
			count++;
			mid = (left + right) /2 ;
			if(arr[mid] == key)
			{
				return count;
			}
			else if(arr[mid] < key )
			{
				left = mid +1;
		
			}
			else
			{
				right = mid -1;
			}
		}
		return count;
	}
	public static void main(String[] args) {
		
	}
}
