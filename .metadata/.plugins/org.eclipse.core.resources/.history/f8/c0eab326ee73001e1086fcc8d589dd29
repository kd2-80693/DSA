package solution02;

import java.util.List;

public class Main {
	public static int linearSearch(List<Employee> list , Employee key )
	{
		for(int i = 0 ; i  < list.size() ; i ++ )
		{
			if(list.get(i).getEmpid()==key.getEmpid())
			{
				System.out.println("Found at "+ i);
				return i;
			}
		}
		return -1;
	}
	
	public static int  binarySearch(List<Employee> list , Employee key )
	{
		
		int left = 0;
		int right = list.size()-1;
		int mid ;
		while(left <= right)
		{
			
			mid = (left + right) /2 ;
			if(list.get(mid).getEmpid()==key.getEmpid())
			{
				return mid;
			}
			else if(list.get(mid).getEmpid() < key.getEmpid())
			{
				left = mid +1;
		
			}
			else
			{
				right = mid -1;
			}
		}
		return count;
	}
	public static void main(String[] args) {
		
	}
}
