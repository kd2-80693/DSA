package solution03;

import java.util.Arrays;

public class Main {
	public static int  binarySearch(int[] arr , int key )
	{
		Arrays.sort(arr);
		int left = 0;
		int right = arr.length-1;
		int mid ;
		while(left <= right)
		{
			
			mid = (left + right) /2 ;
			if(arr[mid] == key)
			{
				return mid;
			}
			else if(arr[mid] < key )
			{
				left = mid +1;
		
			}
			else
			{
				right = mid -1;
			}
		}
		return -1;
	}
	
	int[] arr = {1,2,3,-1,2,1,0,-1,7,8};
	
}
